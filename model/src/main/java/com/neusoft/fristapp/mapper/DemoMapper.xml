<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.neusoft.fristapp.mapper.DemoMapper">

    <resultMap id="user" type="com.neusoft.fristapp.domain.Demo">
        <id column="id" property="id" />
        <result property="name" column="name"></result>
        <result property="createTime" column="create_time"></result>
    </resultMap>
    <select id="queryDemo"  parameterType="int" resultMap="user">
        SELECT id,NAME,CREATE_TIME FROM DEMO D
        WHERE D.ID = #{id}
    </select>

</mapper>